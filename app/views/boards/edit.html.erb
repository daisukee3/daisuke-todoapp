<div class="container">
    <div class="top_label">New Board</div>
    <%= form_with(model: @board, url: board_path(@board), method: 'put', local: true) do |f| %>
    <div class= "title_label">
        <%= f.label :title, 'Name' %>
    </div>
    <div>
        <%= f.text_field :title, class: 'text' %>
    </div>
    <div class= "content_label">
        <%= f.label :content, 'Description' %>
    </div>
    <div>
        <%= f.text_area :content %>
    </div>
    <%= f.submit 'Submit', class: 'btn-primary' %>
    <% end %>
</div>